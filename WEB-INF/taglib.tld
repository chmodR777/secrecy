      get request value;
    </info>
    <attribute>
      <name>name</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  <!-- Prameters Equal -->
  <tag>
    <name>parametersequal</name>
    <tagclass>com.syntc.common.taglib.ParametersEqualTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
      whether the name queals the value;
    </info>
    <attribute>
      <name>name</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <name>value</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
