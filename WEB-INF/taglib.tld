  <!-- ResultHeader -->
  <tag>
    <name>resultheader</name>
    <tagclass>com.syntc.taglib.common.ResultHeaderTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
      Resultset Information.
    </info>
    <attribute>
      <name>resultName</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  <!-- ResultColumn -->
  <tag>
    <name>resultcolumn</name>
    <tagclass>com.syntc.taglib.common.ResultColumnTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>
      get current record column value.
    </info>
    <attribute>
      <name>column</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
	<attribute>
      <name>type</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
